{
  "name": "@kbook/worker",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "lint": "eslint src --ext ts",
    "check": "tsc --noEmit",
    "test": "pnpm lint && pnpm check",
    "build": "npx supabase functions build",
    "dev": "npx supabase functions serve",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:migrate": "npx supabase db push",
    "supabase:deploy": "npx supabase functions deploy --project-ref $SUPABASE_PROJECT_REF --no-verify-jwt"
  },
  "dependencies": {
    "@kbook/shared": "workspace:*",
    "@supabase/supabase-js": "2.76.1",
    "nanoid": "5.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@kbook/config": "workspace:*",
    "@eslint/js": "9.13.0",
    "@types/node": "22.12.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.4.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1"
  }
}
